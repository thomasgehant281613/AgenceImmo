<div class="card md-4" xmlns:font-weight="http://www.w3.org/1999/xhtml" >
    {% if property.filename %}
        <img src="{{ vich_uploader_asset(property, 'imageFile')  }}"alt="card-img-top"; style="width: 100%"; height="230"; />
        {% else %}
        <img src="{{ ' /images/empty/empty.jpg ' }}"alt="card-img-top"; style="width: 100%"; height="230"; />
        {% endif %}
    <div class="card-body">
        <h5 class="card-title">
            <a href="{{ path('property.show', {slug: property.slug, id: property.id}) }}">
                {{ property.title }}
            </a>
        </h5>
        <p class="card-text">{{ property.city }} ({{ property.postalCode }})</p>
        <div class="text-primary" style="font-size: 2rem; font-weight: bold";>{{ property.formattedPrice }} â‚¬</div>
    </div>


</div>